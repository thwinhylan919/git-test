{
    "name": "obdx-build-tool",
    "version": "19.2.0",
    "description": "A tool to build the GUI, require optimization, minify JS, CSS, HTML and compile SASS to CSS and update the resource path variables dynamically",
    "author": "Ravindra Singh",
    "private": true,
    "scripts": {
        "css": "node sass-compile-parallel.js",
        "jsdoc": "rd /s /q ..\\jsdocs & .\\node_modules\\.bin\\jsdoc -c jsdoc.json --readme jsdoc-conf/README.md",
        "jsdoc-nix": "./node_modules/.bin/jsdoc -c jsdoc.json --readme jsdoc-conf/README.md",
        "production": "npm install --only=production"
    },
    "dependencies": {
        "ajv": "6.10.0",
        "chalk": "2.4.2",
        "cheerio": "1.0.0-rc.3",
        "docdash": "1.1.1",
        "eslint": "5.16.0",
        "eslint-plugin-extra-rules": "0.8.1",
        "eslint-plugin-no-storage": "1.0.2",
        "eslint-plugin-requirejs": "4.0.0",
        "esprima": "4.0.1",
        "fs-extra": "8.1.0",
        "grunt": "1.0.4",
        "grunt-combine-media": "1.0.0",
        "grunt-concurrent": "2.3.1",
        "grunt-contrib-clean": "2.0.0",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-cssmin": "3.0.0",
        "grunt-contrib-htmlmin": "3.1.0",
        "grunt-contrib-requirejs": "1.0.0",
        "grunt-cwebp": "2.1.1",
        "grunt-newer": "1.3.0",
        "grunt-sass": "3.0.2",
        "grunt-string-replace": "1.3.1",
        "grunt-svgmin": "^6.0.0",
        "grunt-terser": "0.1.0",
        "https-proxy-agent": "2.2.4",
        "jsdoc": "3.6.3",
        "matchdep": "2.0.0",
        "multi-glob": "1.0.2",
        "node-sass": "4.13.0",
        "rimraf": "2.6.3",
        "time-grunt": "2.0.0",
        "typo-js": "1.0.3",
        "xml2js": "0.4.19",
        "xmldom": "0.1.27"
    },
    "devDependencies": {
        "execa": "1.0.0",
        "js-beautify": "1.10.0",
        "listr": "0.14.3"
    },
    "engines": {
        "node": ">=12.13.1"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ]
}
